    "mongodb": "^2.2.11",
    "morgan": "^1.7.0",
    "protobufjs": "^6.1.0",
    "pug": "^2.0.0-beta6",
    "request": "^2.78.0",
    "stylus": "^0.54.5",
    "winston": "^2.3.0",
    "ioant-proto" : ">=0.1.1",
